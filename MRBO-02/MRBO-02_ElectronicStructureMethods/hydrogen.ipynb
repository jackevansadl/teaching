    "from pyscf import gto\n",
    "mol = gto.Mole()\n",
    "mol.build(atom = '''O 0 0 0; H  0 1 0; H 0 0 1''', basis = 'sto-3g')\n",
    "\n",
    "mol.charge = 0\n",
    "mol.spin = 0 # 2j == nelec_alpha - nelec_beta\n",
    "mol.symmetry = 1\n",
    "\n",
    "mf = mol.apply('RHF').run()"
   ]
  }
